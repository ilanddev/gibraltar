{
  "name": "gibraltar",
  "version": "1.0.0",
  "description": "A library for easily creating a visualization of an iland cloud virtual environment.",
  "main": "./build/gibraltar.js",
  "types": "./build/gibraltar.d.ts",
  "author": "csnyder@iland.com",
  "license": "Apache-2.0",
  "scripts": {
    "info": "npm-scripts-info",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "build": "rm -rf ./build && webpack --mode production",
    "build-demo": "rm -rf ./demo/dist && webpack --mode production --config webpack.demo.config.js",
    "serve-demo": "node --max_old_space_size=6144 node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.demo.config.js --mode development",
    "docs": "typedoc src/gibraltar.ts --mode file --theme minimal --out build/docs"
  },
  "scripts-info": {
    "info": "Display information about the scripts",
    "lint": "Lint all typescript source files",
    "test": "Run unit tests",
    "build": "(Trash and re)build the library",
    "build-demo": "(Trash and re)build the demo application",
    "serve-demo": "Start a local development server for the demo application",
    "docs": "Generate HTML API documentation"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "iland-sdk": "1.x",
    "paper": "0.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/codemirror": "0.0.73",
    "@types/jest": "^24.0.11",
    "@types/node": "7.0.12",
    "@types/paper": "0.11.10",
    "angular-router-loader": "0.8.5",
    "core-js": "^2.5.4",
    "css-loader": "2.1.1",
    "dts-bundle": "0.7.3",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "iland-sdk": "1.0.1",
    "jest": "^29.6.1",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "less": "3.13.1",
    "less-loader": "4.1.0",
    "ng-annotate-loader": "0.6.1",
    "npm-scripts-info": "^0.3.9",
    "paper": "^0.12.0",
    "raw-loader": "^2.0.0",
