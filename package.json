    "ts-loader": "5.3.3",
    "tslib": "1.9.3",
    "tslint": "5.15.0",
    "tslint-config-standard": "8.0.1",
    "typedoc": "0.14.2",
    "typescript": "3.4.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1",
    "zone.js": "~0.8.26"
  }
}
